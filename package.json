{
  "name": "prototype-app",
  "description": "Prototype a web app.",
  "version": "0.0.0-1",
  "author": {
    "name": "Ray Patterson",
    "email": "github.raypatterson@gmail.com"
  },
  "bugs": "https://github.com/raypatterson/prototype-app/issues",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "bootstrap-sass": "~3.3.7",
    "fastclick": "~1.0.6",
    "font-awesome": "~4.7.0",
    "hack-font": "~2.0.19"
  },
  "devDependencies": {
    "@raypatterson/full-metal-webpack": "github:raypatterson/full-metal-webpack",
    "commitizen": "~2.9.5",
    "cz-conventional-changelog": "~1.2.0",
    "eslint": "~3.14.0",
    "eslint-config-xo": "~0.17.0",
    "fixpack": "~2.3.1",
    "npm-autolink": "~0.4.1",
    "ntl": "~1.1.3",
    "updtr": "~0.2.3"
  },
  "engines": {
    "node": ">=6"
  },
  "homepage": "https://github.com/raypatterson/prototype-app/",
  "keywords": [
    "app",
    "prototype",
    "web"
  ],
  "license": "UNLICENSED",
  "main": "index.js",
  "maintainers": [
    {
      "name": "Ray Patterson",
      "email": "github.raypatterson@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:raypatterson/prototype-app.git"
  },
  "scripts": {
    "build": "fmw --clean --production --config ./fmw.production.config.js",
    "build:preview": "npm run build -- --open",
    "commit": "git-cz",
    "dev": "fmw",
    "dev:debug": "npm run dev -- --debug",
    "help": "fmw --help",
    "link": "npm-autolink",
    "link:rm": "npm-autolink remove",
    "ls": "ntl .",
    "test": "npm run test:pass",
    "test:fail": "exit 1",
    "test:pass": "exit 0",
    "upgrade": "updtr && fixpack"
  }
}
